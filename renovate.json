{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "branchPrefix": "feature/renovate/",
    "dryRun": "false",
    "username": "avnes",
    "onboarding": false,
    "platform": "github",
    "repositories": [
        "avnes/ansible-role-vscode"
    ],
    "packageRules": [{
        "matchUpdateTypes": [
            "pin",
            "digest",
            "patch",
            "minor",
            "lockFileMaintenance"
        ],
        "stabilityDays": 0,
        "automerge": true,
        "matchCurrentVersion": "!/^0/",
        "ignoreTests": true,
        "dependencyDashboardApproval": false
    },{
        "matchUpdateTypes": [
            "major"
        ],
        "stabilityDays": 0,
        "automerge": false,
        "dependencyDashboard": true,
        "dependencyDashboardApproval": false
    }]
}
